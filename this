class Athis 
{ 
int a,b,c; 
void met(int t,int p,int m) 
{ 
this.a=m; 
this.b=p; 
this.c=t; 
this.display(); 
} 
void display () 
{ 
System.out. println(a); 
System.out. println(b); 
System.out.println(c);
System.out.println("current class method is invoked using this"); 
} 
} 
class Usethis { public static void main(String args[]) 
{ 
Athis k=new Athis(); 
k.met(10,20,30); 
} 
}
